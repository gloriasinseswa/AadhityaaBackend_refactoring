[run]
source = .
omit = 
    tests/
    migrations/
    */__init__.py
    */settings.py
    */urls.py
    */admin.py
    */apps.py
    manage.py
    venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def setUp
    def test_
